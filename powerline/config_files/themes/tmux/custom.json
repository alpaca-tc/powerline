{
  "default_module": "powerline.segments.common",
  "segment_data": {
    "hostname": {
      "before": "î‚¢ ",
      "args": {
        "only_if_ssh": true
      }
    },
    "uptime": {
      "before": " "
    },
    "external_ip": {
      "before": " "
    },
    "date": {
      "before": " "
